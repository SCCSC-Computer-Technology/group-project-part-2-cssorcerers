@{
	ViewData["Title"] = "F1 Home";
}

<html>
<head>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/Sports css/f1.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/Common css/common.css" asp-append-version="true">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;700&display=swap" rel="stylesheet">
</head>
<body>
    <div class="container">
        <h1>F1 Dashboard</h1>
        <div class="nav-menu">
            <a class="nav-btn" asp-controller="home" asp-action="index">Home</a>
            <a class="nav-btn" asp-controller="f1" asp-action="allconstructors">Constructors</a>
            <a class="nav-btn" asp-controller="f1" asp-action="allcurrentconstructors" asp-route-team="">Current Constructors</a>
            <a class="nav-btn" asp-controller="f1" asp-action="allraces" asp-route-driver="">Races</a>
            <a class="nav-btn" asp-controller="f1" asp-action="alldrivers" asp-route-alldrivers="">Drivers</a>
        </div>
    </div>
</body>
</html>